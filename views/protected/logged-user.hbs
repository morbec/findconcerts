<div class="navbar-container">
  <a class="navbar-link" href="/">home</a>
  <a class="navbar-link" href="/auth/logout">log out</a>
</div>


<div class="user-window">

  <div class="user-profile-name">
    <h2>welcome</h2>
    <h1>{{loggedUser.username}}</h1>
  </div>

  <div class="user-details-container">

    <div class="user-profile-container">

      <img class="user-img" src="https://ca.slack-edge.com/T02CQ4EN4-UDSGJN4UF-67004e236ce4-72" alt="band-img">
      <div class="user-profile-content">
        <p class="event-datevenue">{{loggedUser.lastName}}, {{loggedUser.firstName}}</p>
        <p class="event-location">artists followed: {{loggedUser.artists.length}}</p>
        <p class="event-location">events followed: 0</p>
      </div>
    </div>

    <div class="lineup-detailed-container">

      <p class="lineup-text">artists followed </p>
      <div class="event-lineup-container">
        {{#each artistObjs}}
        <div class="event-lineup-content">
          <a target=_blank href="{{this.facebook_page_url}}">
            <img class="event-lineup-img" src="{{this.thumb_url}}" alt="band-pic">
            <div class="lineup-band-name">
              <p>{{this.name}}</p>
            </div>
        </div>
        </a>
        {{/each}}
      </div>

      <div class="event-detailed-container">
        <p class="lineup-text">events followed </p>
        <div class="event-datiled-dateandtime">
          <p>{{artist.bio.summary}}</p>
        </div>

        {{!-- <div class="event-detailed-venue">
          <p>bio placeholder</p>
          <p class="event-detailed-location">bio placeholder</p>
        </div> --}}
      </div>
    </div>
  </div>
</div>